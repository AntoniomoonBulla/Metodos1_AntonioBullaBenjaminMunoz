        "import numpy as np\n",
        "from tqdm import tqdm\n",
        "\n",
        "N = int(1e6)\n",
        "θ = 2\n",
        "n = 20\n",
        "puntos = np.zeros([N,n])\n",
        "\n",
        "for i in tqdm(range(N)):\n",
        "  puntos[i] = (np.random.exponential(θ,n))\n",
        "Promedio = np.mean(puntos, axis=1)\n",
        "var_ = np.var(Promedio, ddof=1)\n",
        "\n",
        "var_"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "print(θ**2/n)"
